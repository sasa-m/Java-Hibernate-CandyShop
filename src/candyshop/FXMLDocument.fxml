<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="pane" prefHeight="350.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="candyshop.FXMLDocumentController">
    <children>
        <Button fx:id="btSave" layoutX="122.0" layoutY="229.0" onAction="#saveButtonAction" prefHeight="30.0" prefWidth="80.0" text="Save">
         <font>
            <Font name="Tahoma" size="14.0" />
         </font></Button>
        <Label fx:id="label" layoutX="14.0" layoutY="14.0" minHeight="16" minWidth="69" prefHeight="32.0" prefWidth="155.0" text="Candy Shop" textFill="#a10707">
         <font>
            <Font name="Tahoma Bold" size="24.0" />
         </font></Label>
      <Label layoutX="11.0" layoutY="121.0" prefHeight="18.0" prefWidth="54.0" text="Name">
         <font>
            <Font name="Tahoma" size="14.0" />
         </font>
      </Label>
      <Label layoutX="11.0" layoutY="163.0" prefHeight="18.0" prefWidth="54.0" text="Price">
         <font>
            <Font name="Tahoma" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="tfName" layoutX="76.0" layoutY="116.0">
         <font>
            <Font name="Tahoma" size="14.0" />
         </font>
      </TextField>
      <TextField fx:id="tfPrice" layoutX="77.0" layoutY="158.0" prefHeight="25.0" prefWidth="170.0">
         <font>
            <Font name="Tahoma" size="14.0" />
         </font>
      </TextField>
      <TableView fx:id="table" layoutX="300.0" layoutY="87.0" prefHeight="170.0" prefWidth="348.0">
        <columns>
          <TableColumn fx:id="colId" prefWidth="48.0" text="Id" />
          <TableColumn fx:id="colName" prefWidth="207.0" text="Name" />
            <TableColumn fx:id="colPrice" prefWidth="89.0" text="Price" />
        </columns>
      </TableView>
      <Button fx:id="btDelete" layoutX="666.0" layoutY="227.0" mnemonicParsing="false" onAction="#deleteButtonAction" prefHeight="30.0" prefWidth="80.0" text="Delete">
         <font>
            <Font name="Tahoma" size="14.0" />
         </font>
      </Button>
      <Button fx:id="btClose" layoutX="712.0" layoutY="314.0" mnemonicParsing="false" onAction="#closeButtonAction" prefHeight="30.0" prefWidth="80.0" text="Close">
         <font>
            <Font name="Tahoma" size="14.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
